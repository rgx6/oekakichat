extends layout
block script
    script(type="application/javascript", src="/javascripts/jquery-2.1.0.min.js")
    script(type="application/javascript", src="/javascripts/bootstrap.min.js")
    script(type="application/javascript", src="/javascripts/my.js")
    script(type="application/javascript", src="/javascripts/AddressBook.js")
block link
    link(type="text/css", rel="stylesheet", href="/stylesheets/bootstrap.min.css")
block body
    div.container(style="max-width: none; important; width: 970px;")
        div.row
            div.col-xs-12
                div.clearfix
                    h2.pull-left アドレス帳
                    button#import.btn.btn-default.pull-right(type="button") インポート
                    button#export.btn.btn-default.pull-right(type="button") エクスポート
                p#message.bg-danger
                table#roomList.table.table-striped.table-hover
                    tbody
